  "workbench.settings.editor": "json",

  // Editor
  "editor.acceptSuggestionOnEnter": "on",
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.fontFamily": "Iosevka Term",
  "editor.fontLigatures": true,
  // "editor.fontSize": 18,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.letterSpacing": 1,
  // "editor.lineHeight": 28,
  "editor.lineHeight": 24,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.semanticTokenColorCustomizations": {
    "[Ayu Dark Bordered]": {
      // Apply to this theme only
      "enabled": true
      // "rules": {
      //     "magicFunction:python": "#ee0000",
      //     "function.declaration:python": "#990000",
      //     "*.decorator:python": "#0000dd",
      //     "*.typeHint:python": "#5500aa",
      //     "*.typeHintComment:python": "#aaaaaa"
      // }
    },
    "[Material Theme Darker High Contrast]": {
      "enabled": true
    }
  },
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.tokenColorCustomizations": {
    "[Material Theme Darker High Contrast]": {
      "textMateRules": [
        {
          "scope": [
            "comment",
            "punctuation.definition.comment",
            "string.quoted.docstring"
          ],
          "settings": {
            "foreground": "#757575"
          }
        }
      ]
    }
  },

  // Terminal
  "terminal.external.linuxExec": "alacritty",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  // "terminal.integrated.fontFamily": "",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.letterSpacing": 1.0,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.defaultProfile.linux": "tmux",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    }
  },
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "zsh"
    }
  },

  // SQL tools
  "sqltools.autoOpenSessionFiles": false,
  "sqltools.codelensLanguages": ["pl/sql", "sql"],
  "sqltools.completionLanguages": ["pl/sql", "sql"],
  "sqltools.format": {
    "language": "pl/sql",
    "linesBetweenQueries": 2,
    "reservedWordCase": "upper"
  },
  "sqltools.formatLanguages": ["pl/sql", "sql"],
  "sqltools.highlightQuery": false,
  "sqltools.results": {
    "customization": {
      "font-family": "JetBrains Mono",
      "font-size": "12"
    }
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "askForPassword": true,
      "driver": "PostgreSQL",
      "name": "bachdb",
      "connectString": "postgresql://cloud:scape@localhost:5432/bachdb"
    }
  ],

  // Telemetry
  "extensions.ignoreRecommendations": true,
  "npm.fetchOnlinePackageInfo": false,

  // Debugger
  "debug.allowBreakpointsEverywhere": true,

  // Search
  "search.showLineNumbers": true,

  // Zen Mode
  "zenMode.centerLayout": true,
  "zenMode.fullScreen": true,
  "zenMode.hideActivityBar": true,
  "zenMode.hideLineNumbers": false,

  /* **************** Extensions Settings **************** */

  // Better Comments
  "better-comments.tags": [
