    },
    {
      "tag": "question",
      "color": "#FE0100",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
    // {
    //   "tag": "*",
    //   "color": "#98C379",
    //   "strikethrough": false,
    //   "underline": false,
    //   "backgroundColor": "transparent",
    //   "bold": false,
    //   "italic": false
    // }
  ],

  // Code Spell Checker
  "cSpell.enableFiletypes": ["toml"],

  // Even Better TOML
  "evenBetterToml.formatter.alignComments": true,
  "evenBetterToml.formatter.alignEntries": false,
  // "evenBetterToml.formatter.columnWidth": 119,
  "evenBetterToml.formatter.columnWidth": 400,
  "evenBetterToml.formatter.compactArrays": true,
  "evenBetterToml.formatter.compactInlineTables": false,
  "evenBetterToml.formatter.indentTables": false,
  "evenBetterToml.formatter.reorderKeys": false,
  "evenBetterToml.semanticTokens": true,
  // Git
  "gitlens.defaultDateStyle": "absolute",
  "scm.diffDecorations": "all",

  // Material Icon Theme
  "material-icon-theme.folders.color": "#42a5f5",
  "material-icon-theme.folders.theme": "specific",

  // Red Hat Telemetry
  "redhat.telemetry.enabled": false,

  // Rust Analyzer
  // "rust-analyzer.cargo.features": "all",
  // "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.completion.autoimport.enable": true,
  "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
  "rust-analyzer.imports.granularity.enforce": true,
  "rust-analyzer.imports.granularity.group": "crate",
  "rust-analyzer.imports.prefix": "crate",

  /* **************** Language Extension Settings **************** */

  // Go
  "go.useLanguageServer": true,

  // Java
  "java.debug.settings.console": "integratedTerminal",
  "java.debug.logLevel": "verbose",
  "java.configuration.checkProjectSettingsExclusions": false,
  "python.languageServer": "Pylance",

  /* **************** Language Settings **************** */

  "[asciidoc]": {
    "editor.renderFinalNewline": "on",
    "editor.trimAutoWhitespace": true,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 180,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },

  // "[c]": {
  //   "editor.defaultFormatter": "ms-vscode.cpptools"
  // },

  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"]
  },

  // "[dockercompose]": {
  //   "editor.autoIndent": "advanced",
  //   "editor.defaultFormatter": "ms-azuretools.vscode-docker",
  //   "editor.insertSpaces": true,
  //   "editor.quickSuggestions": {
  //     "other": true,
  //     "comments": false,
  //     "strings": true
  //   },
  //   "editor.tabSize": 2
  // },

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"]
  },

  "[ignore]": {
    "editor.renderFinalNewline": "on",
    "editor.trimAutoWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },

  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },

  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.renderFinalNewline": "on",
    "editor.rulers": [80],
    "editor.trimAutoWhitespace": true,
    // "editor.wordWrap": "bounded",
    "editor.wordWrap": "on",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },

  "[python]": {
    "editor.renderFinalNewline": "on",
    "editor.rulers": [88],
    "editor.trimAutoWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },

  "[plaintext]": {
    "editor.renderFinalNewline": "on",
    "editor.trimAutoWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },

  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.renderFinalNewline": "on",
    "editor.rulers": [/* 79, */ 99],
    "editor.trimAutoWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },

  "[shellscript]": {
    "editor.tabSize": 2,
    "files.eol": "\n",
    "editor.defaultFormatter": "mads-hartmann.bash-ide-vscode"
  },

  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools",
    "editor.renderFinalNewline": "on",
    "editor.trimAutoWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },

  "[toml]": {
    "editor.renderFinalNewline": "on",
    "editor.trimAutoWhitespace": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true
  },

  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },

  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },

  "[yaml]": {
    "editor.autoIndent": "keep",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.insertSpaces": true,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.tabSize": 2,
    "gitlens.codeLens.scopes": ["document"]
  },
  // "files.autoSaveDelay": 100,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "window.commandCenter": true,
  "yaml.schemas": {
    "file:///Users/narayanbhat/.vscode/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "atlascode.jira.lastCreateSiteAndProject": {
    "siteId": "0f19b229-4ef2-49b4-9efc-a90840362881",
    "projectKey": "ORCA"
  },
  // "files.autoSave": "afterDelay",
  "githubPullRequests.pullBranch": "never",
  "docker.environment": {
    "DOCKER_HOST": "unix:///run/user/1000/podman/podman.sock"
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "window.zoomLevel": 1,
  "materialTheme.accent": "Teal",
  "editor.largeFileOptimizations": false,
  "workbench.colorTheme": "Ayu Mirage",
  "workbench.iconTheme": "vscode-icons",
  "githubPullRequests.fileListLayout": "flat",
  "git.openRepositoryInParentFolders": "never",
  "diffEditor.experimental.showMoves": true,
  "rust-analyzer.cargo.noDefaultFeatures": true,
  "rust-analyzer.cargo.features": ["v2", "customer_v2", "payment_methods_v2"],
  // "rust-analyzer.cargo.features": ["recon"],
  "shellformat.flag": "--indent 2 --binary-next-line --case-indent --space-redirects",
  "workbench.editor.highlightModifiedTabs": true,
  "liveServer.settings.donotShowInfoMsg": true
}
