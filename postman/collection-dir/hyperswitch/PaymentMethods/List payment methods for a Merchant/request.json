{
  "method": "GET",
  "header": [
    {
      "key": "Accept",
      "value": "application/json"
    }
  ],
  "url": {
    "raw": "{{baseUrl}}/account/payment_methods",
    "host": ["{{baseUrl}}"],
    "path": ["account", "payment_methods"],
    "query": [
      {
        "key": "client_secret",
        "value": "{{client_secret}}",
        "disabled": true
      }
    ]
  },
  "description": "To filter and list the applicable payment methods for a particular merchant id."
}
