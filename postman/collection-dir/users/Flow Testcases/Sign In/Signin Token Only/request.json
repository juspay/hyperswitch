{
  "method": "POST",
  "header": [
    {
      "key": "Content-Type",
      "value": "application/json"
    },
    {
      "key": "Cookie",
      "value": "Cookie_1=value"
    }
  ],
  "body": {
    "mode": "raw",
    "raw_json_formatted": {
      "email": "{{user_email}}",
      "password": "{{user_password}}"
    }
  },
  "url": {
    "raw": "{{baseUrl}}/user/v2/signin?token_only=true",
    "host": [
      "{{baseUrl}}"
    ],
    "path": [
      "user",
      "v2",
      "signin"
    ],
    "query": [
      {
        "key": "token_only",
        "value": "true"
      }
    ]
  }
}
